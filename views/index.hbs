
<h1>FitFun</h1>
<ul>
  {{#each users}}
    <a href="/users/{{this._id}}"><li>Name: {{this.name}}</li></a>
    
    <br>
    <div><a href="/users/{{this._id}}/edit">Edit</a></div>
    <form action="/users/{{this._id}}">
      <input type="submit" value="DELETE"/>
    </form>
    <hr>
  {{/each}}
</ul>


<button id="myButton" class="float-center submit-button" >SIGN UP NOW!</button>
<button>See users!</button>
<script type="text/javascript">
    document.getElementById("myButton").onclick = function () {
        location.href = "/new";
    };
</script>


